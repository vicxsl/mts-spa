<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.qisen.mts.spa.dao.SpaGoodsType">
	<resultMap id="BaseResultMap" type="com.qisen.mts.spa.model.entity.SpaGoodsType">
		<id column="Id" property="id" jdbcType="INTEGER" />
		<result column="TypeName" property="typeName" jdbcType="VARCHAR" />
		<result column="TypeNo" property="typeNo" jdbcType="VARCHAR" />
		<result column="Status" property="status" jdbcType="CHAR" />
		<result column="eid" property="eid" jdbcType="INTEGER" />
		<result column="sid" property="sid" jdbcType="INTEGER" />
	</resultMap>
	<sql id="Base_Column_List">
		Id, TypeName, TypeNo, Status,sid,eid
	</sql>
	<select id="selectByPrimaryKey" resultMap="BaseResultMap"
		parameterType="java.lang.Integer">
		select
		<include refid="Base_Column_List" />
		from goodsType
		where Id = #{id,jdbcType=INTEGER}
	</select>

	<select id="selectGoodsTypes" resultMap="BaseResultMap">
		select * from goodsType
		<where>
			1=1
			<if test="id != null">
				and id=#{id}
			</if>
			<if test="typeName != null">
				<bind name="parttern_name" value="'%'+typeName+'%'" />
				and name like #{parttern_name}
			</if>
			<if test="sid != null">
				and sid=#{sid}
			</if>
			<if test="eid != null">
				and eid = #{eid}
			</if>
			<if test="typeNo != null">
				and TypeNo=#{typeNo}
			</if>
			<if test="status != null">
				and Status = #{status}
			</if>
		</where>


	</select>

	<update id="updateByPrimaryKey">
		update goodsType
		<set>
			<if test="typeName != null and typeName !=''">
				TypeName = #{typeName},
			</if>
			<if test="typeNo != null and typeNo !=''">
				TypeNo = #{typeNo},
			</if>
			<if test="status != null and status !=''">
				Status = #{status},
			</if>
		</set>
		<where>
			eid = #{eid} and sid = #{sid} and id = #{id}
		</where>
	</update>

	<delete id="deleteByPrimaryKey" parameterType="java.lang.Integer">
		delete from
		goodsType
		where Id = #{id,jdbcType=INTEGER}
	</delete>
	<insert id="insert" parameterType="com.qisen.mts.spa.model.entity.SpaGoodsType">
		insert into goodsType (Id,
		TypeName, TypeNo,
		Status,eid,sid)
		values (#{id,jdbcType=INTEGER},
		#{typeName,jdbcType=VARCHAR},
		#{typeNo,jdbcType=VARCHAR},
		#{status,jdbcType=CHAR},#{eid,jdbcType=INTEGER},#{sid,jdbcType=INTEGER})
	</insert>
</mapper>