<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qisen.mts.spa.dao.ShopDao">

	<!-- 通过手机号码检测账号是否存在 -->
	<select id="queryByAppId" resultType="com.qisen.mts.spa.model.entity.SpaShop"
		parameterType="String">
		<![CDATA[
			select *  from  shops
		]]>
		<where>
			<![CDATA[
				 appid = #{appid} 
			]]>
		</where>
	</select>
	
	
	
	<!-- 通过手机号码检测账号是否存在 -->
	<select id="shopsImgList" resultType="com.qisen.mts.spa.model.entity.SpaImg" parameterType="com.qisen.mts.spa.model.entity.SpaImg">
		<![CDATA[
			select *  from  imgTable
		]]>
		<where>
			<![CDATA[
				 appid = #{appid} and eid = #{eid} and status = "0"
			]]>
		</where>
	</select>
</mapper>
