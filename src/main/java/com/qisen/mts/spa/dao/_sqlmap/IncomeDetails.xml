<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.qisen.mts.spa.dao.IncomeDetailsDao">

	<!-- 查询获利详情 -->
	<select id="list" resultType="com.qisen.mts.spa.model.entity.SpaIncomeDetails" parameterType="com.qisen.mts.spa.model.entity.SpaIncomeDetails">
		<![CDATA[
			select *  from  incomeDetails 
		]]>
		<where>
			<![CDATA[
				 appid = #{appid} and openid = #{openid} and eid = #{eid} 
			]]>
			<if test="createTime != null and createTime !=''">
				<![CDATA[
					and createTime > #{createTime} 
				]]>
			</if>
		</where>
		<![CDATA[
					order by createTime 
				]]>
	</select>

</mapper>
